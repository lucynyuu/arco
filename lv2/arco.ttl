@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .

<https://example.org/plugins/arco>
        a lv2:Plugin ;
        doap:name "Arco" ;
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:project <http://lv2plug.in/ns/lv2> ;
        doap:maintainer "Lucy Nyuu" ;
        doap:developer "Lucy Nyuu" ;
        lv2:requiredFeature urid:map ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:InputPort ,
                atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 0 ;
                lv2:symbol "in" ;
                lv2:name "In"
        ] , [
                a lv2:OutputPort ,
                atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 1 ;
                lv2:symbol "out" ;
                lv2:name "Out"
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "arp_speed" ;
                lv2:name "Arpeggiator Speed" ;
                lv2:default 0.5 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "arp_reverse" ;
                lv2:name "Arpeggiator Direction" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "arp_enable" ;
                lv2:name "Arpeggiator Enabled" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "chord_enable" ;
                lv2:name "Chord Generator Enabled" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "chord" ;
                lv2:name "Chord" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 2
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "octave" ;
                lv2:name "Octave" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 4
        ] , [
                a lv2:InputPort , lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "arp_pattern" ;
                lv2:name "Arpeggiator Pattern" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 2
        ] .
